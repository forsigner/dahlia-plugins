  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "customize-cra": "^0.2.11"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "jest": "^23.6.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.0.2",
